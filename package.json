    "devDependencies": {
        "babel-eslint": "7.2.3",
        "concurrently": "^3.5.1",
        "eslint": "^4.19.1",
        "eslint-config-airbnb": "^16.1.0",
        "eslint-plugin-import": "^2.10.0",
        "eslint-plugin-jsx-a11y": "^6.0.3",
        "eslint-plugin-react": "^7.7.0",
        "supertest": "^3.0.0",
        "tap-spec": "^4.1.1",
        "tape": "^4.8.0"
    },
    "dependencies": {
